{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "ESNext",
    "module":"ESNext",
    "lib": ["ESNext"],
    "skipLibCheck": true,
    "strict": true,
    "sourceMap": false,
    "strictPropertyInitialization": false,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true
  }
}
