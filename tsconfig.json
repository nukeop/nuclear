{
  "compilerOptions": {
    "jsx": "react",
    "moduleResolution": "node",
    "target": "ES2018",
    "module":"ESNext",
    "lib": ["ESNext"],
    "skipLibCheck": true,
    "strict": true,
    "sourceMap": true,
    "strictPropertyInitialization": false,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noImplicitAny": false
  },
  "exclude": [
    "dist"
  ]
}
